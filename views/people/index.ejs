<div class="row">
    <a href='/people/add' class='btn btn-primary btn-block'>Add Person</a>
    <table class='table table-hover table-striped table-bordered'>
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Age</th>
            <th>Edit</th>
        </tr>
        <% for(let i=0; i < people.length; i++) { %>
            <tr>
                <td><%= people[i].Id %></td>
                <td><%= people[i].FirstName %></td>
                <td><%= people[i].LastName %></td>
                <td><%= people[i].Age %></td>
                <td>
                    <a href='/people/edit?id=<%= people[i].Id %>' class='btn btn-warning'>Edit</a>
                </td>
            </tr>
        <%}%>

    </table>

</div>